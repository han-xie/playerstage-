# Create a Stage driver and attach position2d and laser interfaces 
# to the model "r0"
driver
( 
  name "stage"
  provides [ "position2d:0"]
  model "r0"
  pos2dtype "POS2D4TIRES" 
)

driver
(
name "stage"
provides [ "position2d:1" "laser:0"]
model "l0"
pos2dtype "FPOS2D4TIRES"
requires ["position2d:0"]
alwayson 1
)
